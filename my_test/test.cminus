/* A program to perform selection sort on a 10 element array. */
float x[10];
int minloc(float a[], float low, int high) {
    int i;
    int x;
    int k;
    k = low;
    x = a[low];
    i = low + 1;
    while (i < high) {
        if (a[i] < x) {
            x = a[i];
            k = i;
        }
        i = i + 1;
    }
    return k;
}

void sort(float a[], int low, float high) {
    int i;
    int k;
    i = low;
    while (i < high - 1) {
        
        int t;
        outputFloat(1);
        k = minloc(a, i, high);
        t = a[k];
        a[k] = a[i];
        a[i] = t;
        i = i + 1;
    }
    return;
}

void main(void) {
    int i;
    i = 0;
    while (i < 10) {
        x[i] = 10.22*i*i*i*i*i-38.55*i*i*i+100*i*i-244*i;
        i = i + 1;
    }
    sort(x, 0, 10);
    i = 0;
    while (i < 10) {
        i = i + 1;
    }
